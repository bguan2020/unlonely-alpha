{
  "expo": {
    "name": "Unlonely",
    "scheme": "unlonely",
    "slug": "unlonely",
    "backgroundColor": "#000000",
    "orientation": "portrait",
    "splash": {
      "backgroundColor": "#000000",
      "image": "./splash.png"
    },
    "version": "1.0.0",
    "web": {
      "bundler": "metro"
    },
    "ios": {
      "bundleIdentifier": "app.unlonely.mobile",
      "infoPlist": {
        "UIBackgroundModes": ["audio"],
        "NSNotificationsUsageDescription": "Unlonely would like to send you notifications about streams going live and new NFCs being published.",
        "NSUserNotificationAlertStyle": "banner"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "mode": "development",
          "androidMode": "development",
          "iosMode": "development"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d3a8a2ee-d6c1-4d55-9e6b-b732b0802275"
      }
    }
  }
}
