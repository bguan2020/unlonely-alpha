{
  "expo": {
    "name": "Unlonely",
    "scheme": "unlonely",
    "slug": "unlonely",
    "backgroundColor": "#000000",
    "userInterfaceStyle": "dark",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "backgroundColor": "#000000",
      "image": "./assets/splash.png"
    },
    "version": "1.0.0",
    "web": {
      "bundler": "metro"
    },
    "ios": {
      "bundleIdentifier": "app.unlonely.mobile",
      "infoPlist": {
        "UIBackgroundModes": ["audio"],
        "NSUserNotificationAlertStyle": "banner",
        "UIUserInterfaceStyle": "dark"
      },
      "userInterfaceStyle": "dark"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "mode": "development",
          "androidMode": "development",
          "iosMode": "development",
          "sounds": ["./assets/sounds/live.wav", "./assets/sounds/nfc.wav"]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d3a8a2ee-d6c1-4d55-9e6b-b732b0802275"
      }
    },
    "android": {
      "package": "app.unlonely.mobile",
      "googleServicesFile": "./google-services.json",
      "userInterfaceStyle": "dark",
      "icon": "./assets/adaptive-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-top.png",
        "backgroundImage": "./assets/adaptive-icon-bottom.png"
      }
    }
  }
}
