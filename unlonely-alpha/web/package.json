{
  "description": "This example shows how to build live cursors along chat and reactions with Liveblocks and Next.js.",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel:install": "cd .. && yarn install; cd ./web && yarn install;",
