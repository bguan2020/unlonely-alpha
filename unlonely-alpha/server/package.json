    "ably": "^1.2.29",
    "apollo-boost": "^0.4.9",
    "apollo-server": "3.6.2",
    "apollo-server-express": "^3.6.2",
    "axios": "^0.27.2",
    "canonicalize": "^1.0.8",
    "config": "^3.3.7",
